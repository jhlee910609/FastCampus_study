package com.junhee.java.city.hospital;

import com.junhee.java.city.design.People;

public class Doctor extends People {
	
	public String myPatient;

	public void diagnosis(String patient) {
		System.out.println(patient + "님을 진찰합니다.");
	}
	
	public void getMyPatient(String patient) {
		this.myPatient = patient;
	}

}
